<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Domain.Voluntar"
           table="voluntar">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property
                name="nume"
                type="text"
                column="nume"/>
        <property
                name="prenume"
                type="text"
                column="prenume"/>
        <property
                name="email"
                type="text"
                column="email"/>

        <property
                name="parola"
                type="text"
                column="parola"/>



    </class>
</hibernate-mapping>