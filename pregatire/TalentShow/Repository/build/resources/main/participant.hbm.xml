<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Domain.Participant"
           table="participant">
        <id name="id" column="id" >
            <generator class="increment"/>
        </id>
        <property
                name="nume"
                type="text"
                column="nume"/>
        <property
                name="status"
                type="text"
                column="status"/>


    </class>
</hibernate-mapping>