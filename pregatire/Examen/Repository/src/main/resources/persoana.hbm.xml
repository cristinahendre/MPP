<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Domain.Jucator"
           table="jucator">
        <id name="id" column="id" >
            <generator class="increment"/>
        </id>
        <property
                name="parola"
                type="text"
                column="parola"/>
        <property
                name="email"
                type="text"
                column="email"/>


    </class>
</hibernate-mapping>